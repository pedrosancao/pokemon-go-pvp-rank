{
  "name": "pokemon-go-pvp-rank",
  "version": "0.2.1",
  "description": "Pokémon Go PVP ranking calculator",
  "keywords": [
    "pokemon-go"
  ],
  "author": "Pedro Sanção <pedrosancao@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pedrosancao/pokemon-go-pvp-rank.git"
  },
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "build": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "start": "ts-node-dev --respawn src/index.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/types": "^7.12.12",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "^\\./(level-constants)\\.js$": "<rootDir>/src/$1",
      "^\\./(pokedex)\\.js$": "<rootDir>/src/$1",
      "^\\./(rank-calculator)\\.js$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.ts$": "babel-jest"
    }
  }
}
